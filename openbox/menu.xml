<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/3.4/menu">
	<menu id="apps-term-menu" label="Apps">
		<item label="Alsamixer">
			<action name="Execute">
				<execute>alsamixergui</execute>
			</action>
		</item>
		<item label="emelfm2">
			<action name="Execute">
				<execute>emelfm2</execute>
			</action>
		</item>
		<item label="google-chrome">
			<action name="Execute">
				<execute>google-chrome</execute>
			</action>
		</item>
		<item label="Rxvt Unicode">
			<action name="Execute">
				<command>urxvt</command>
			</action>
		</item>
	</menu>
	<menu id="system-menu" label="System">
		<item label="obmenu">
			<action name="Execute">
				<execute>obmenu</execute>
			</action>
		</item>
		<item label="Openbox Configuration Manager">
			<action name="Execute">
				<command>obconf</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Reconfigure Openbox">
			<action name="Reconfigure"/>
		</item>
		<item label="Log Out">
			<action name="Exit"/>
		</item>
	</menu>
	<menu id="root-menu" label="Openbox 3">
		<menu id="apps-term-menu"/>
		<menu id="system-menu"/>
	</menu>
</openbox_menu>
